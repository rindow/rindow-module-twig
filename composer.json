{
    "name": "rindow/rindow-module-twig",
    "type": "library",
    "description": "Rindow compatible module for Twig",
    "keywords": ["rindow","framework","web","view","template","twig"],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.3",
        "twig/extensions": "1.*"
    },
    "require-dev": {
        "twig/extensions": "1.*",
        "rindow/rindow-stdlib": "1.*",
        "rindow/rindow-container": "1.*",
        "rindow/rindow-web-http": "1.*",
        "rindow/rindow-web-router": "1.*",
        "rindow/rindow-web-form": "1.*",
        "rindow/rindow-web-mvc": "1.*"
    },
    "suggest": {
        "rindow/rindow-web-mvc": "When using with rindow-web-mvc",
        "rindow/rindow-web-form": "When using web-form extensions"
    },
    "autoload": {
        "psr-0": {
            "Rindow\\Module\\Twig\\": "src/"
        }
    }
}
